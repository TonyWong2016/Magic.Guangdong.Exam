{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "layui@2.8.11",
      "destination": "wwwroot/lib/layui/"
    },
    {
      "provider": "cdnjs",
      "library": "axios@1.6.8",
      "destination": "wwwroot/lib/axios/"
    },
    {
      "library": "crypto-js@4.2.0",
      "destination": "wwwroot/plugins/crypto-js/"
    },
    {
      "library": "js-md5@0.8.3",
      "destination": "wwwroot/plugins/js-md5/"
    },
    {
      "provider": "cdnjs",
      "library": "tinymce@7.0.0",
      "destination": "wwwroot/plugins/tinymce/"
    },
    {
      "provider": "cdnjs",
      "library": "echarts@5.5.0",
      "destination": "wwwroot/plugins/echarts/"
    },
    {
      "provider": "cdnjs",
      "library": "microsoft-signalr@8.0.0",
      "destination": "wwwroot/plugins/microsoft-signalr/"

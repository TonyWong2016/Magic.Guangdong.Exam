<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - 广东教育协会</title>
    
    <link type="image/x-icon" rel="shortcut icon" href="/img/guangdong_logo-32.ico" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <link href="~/lib/pico2/css/pico.jade.min.css" rel="stylesheet" />
    <script src="~/lib/pico2/js/modal.min.js"></script>
    <script src="~/lib/axios/axios.min.js"></script>
    <script src="~/web/base.js" asp-append-version="true"></script>
    <script src="~/web/requestapi.js" asp-append-version="true"></script>
</head>
<body>
    <header class="container" style="padding: 0;">
        <img src="~/img/header-ht-high.jpg" />
    </header>
    @RenderBody()
    <footer class="container page-footer" style="padding: 0;">
        <img src="~/img/footer-ht-high.jpg" />
    </footer>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
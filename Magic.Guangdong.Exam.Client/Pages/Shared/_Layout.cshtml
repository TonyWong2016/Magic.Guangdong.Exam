<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - 广东教育协会</title>

    <link type="image/x-icon" rel="shortcut icon" href="/img/guangdong_logo-32.ico" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <link href="~/lib/pico2/css/pico.indigo.min.css" rel="stylesheet" />
    <link href="~/lib/pico2/css/pico.colors.min.css" rel="stylesheet" />
    <script src="~/lib/pico2/js/modal.min.js"></script>
    <script src="~/lib/axios/axios.min.js"></script>
    <link href="~/plugins/toastify/toastify.css" rel="stylesheet" />
    <script src="~/plugins/toastify/toastify.js"></script>
    <script src="~/web/base.js" asp-append-version="true"></script>
    <script src="~/web/requestapi.js" asp-append-version="true"></script>
    <script>
        let requestToken = $('@Html.AntiForgeryToken()').val();

    </script>
</head>
<body>

    <header>
        @* <img src="~/img/header-ht-high.jpg" /> *@

        <nav style="padding:15px">
            <ul>
                <li><strong>Acme Corp</strong></li>
            </ul>
            <ul>
                <li id="AnswerCard"><a href="#" class="secondary">答题卡</a></li>
                <li>
                    <details class="dropdown">
                        <summary>
                            我的账户
                        </summary>
                        <ul dir="rtl">
                            <li><a href="#">个人中心</a></li>
                            <li><a href="#">缴费记录</a></li>
                            <li><a href="#">我的发票</a></li>
                            <li><a href="#">退出</a></li>
                        </ul>
                    </details>
                </li>
            </ul>
        </nav>
    </header>
    

    @RenderBody()
    <footer class="page-footer" style="padding: 0;">
        <img src="~/img/footer-ht-high.jpg" />
    </footer>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
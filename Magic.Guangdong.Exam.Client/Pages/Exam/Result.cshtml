                        <h1 style="color:#e53935;">@Model.Score 分</h1>

                if (Model.OpenResult == DbServices.Entities.PaperOpenResult.YesAndDetail)
                {
                    <a href="/exam/detail?urid=@Model.Urid">
                        查看详情

                if (Model.Marked == 2)
                {
                    <small>包含主观题得分</small>
                }
            }
            else
            {
                        <h3>提示</h3>
                        <h1 style="color:#e53935;" id="mainTip">已完成考试</h1>
                //不让查成绩
            }
        }
        @{
